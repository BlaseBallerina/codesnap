{
  'env': {
    'browser': true,
    'node': true,
    'mocha': true,
    'jquery': true
  },
  'rules': {
   'no-mixed-requires': 0,
   'no-undef': 0,
   'no-underscore-dangle': 0,
   'new-cap' : 0,
   'quotes': 0,
   'strict': 0,
   'eol-last': 0,
   'camelcase': 0,
   'no-unused-vars': 0,
   'no-use-before-define': 0,
   'no-multi-str': 0,
   'space-infix-ops': 0,
   'no-shadow': 1,
   'no-loop-func': 0,
   no-extra-boolean-cast: 0
  }
}
